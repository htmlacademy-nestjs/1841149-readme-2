{
  "name": "@project/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:api-gateway": "npx nx run api-gateway:serve",
    "start:file-storage": "npx nx run user:file-storage",
    "start:notification": "npx nx run notification:serve",
    "start:post": "npx nx run post:serve",
    "start:user": "npx nx run user:serve",
    "all:start": "npx nx run-many -t serve -p api-gateway,user,post,notification,file-storage",
    "file-storage:docker-up": "docker compose --file ./apps/file-storage/docker-compose.dev.yml --project-name readme-file-storage --env-file ./apps/file-storage/file-storage.env up -d",
    "file-storage:docker-down": "docker compose --file ./apps/file-storage/docker-compose.dev.yml --project-name readme-file-storage --env-file ./apps/file-storage//file-storage.env down",
    "notification:docker-up": "docker compose --file ./apps/notification/docker-compose.dev.yml --env-file ./apps/notification/.env --project-name readme-notify up -d",
    "notification:docker-down": "docker compose --file ./apps/notification/docker-compose.dev.yml --env-file ./apps/notification/.env --project-name readme-notify down",
    "post:docker-up": "docker compose --file ./apps/post/docker-compose.dev.yml --env-file ./apps/post/post.env --project-name readme-post up -d",
    "post:docker-down": "docker compose --file ./apps/post/docker-compose.dev.yml --env-file ./apps/post/post.env --project-name readme-post down",
    "user:docker-up": "docker compose --file ./apps/user/docker-compose.dev.yml --env-file ./apps/user/.env --project-name readme-user up -d",
    "user:docker-down": "docker compose --file ./apps/user/docker-compose.dev.yml --env-file ./apps/user/.env --project-name readme-user down",
    "all:docker-up": "npm run file-storage:docker-up && npm run notification:docker-up && npm run post:docker-up && npm run user:docker-up",
    "all:docker-down": "npm run file-storage:docker-down && npm run notification:docker-down && npm run post:docker-down && npm run user:docker-down",
    "lint": "npx nx run-many --target=lint --all",
    "post-db-lint": "npx nx run post:db:lint",
    "post-db-migrate": "npx nx run post:db:migrate",
    "post-db-reset": "npx nx run post:db:reset",
    "post-db-generate": "npx nx run post:db:generate",
    "post-db-fill": "npx nx run post:db:fill"
  },
  "private": true,
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^6.0.2",
    "@nestjs-modules/mailer": "1.10.3",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.14.0",
    "@types/bcrypt": "^6.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "axios": "^1.6.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "0.14.0",
    "dayjs": "^1.11.13",
    "fs-extra": "^11.3.1",
    "handlebars": "4.7.8",
    "mime-types": "^3.0.1",
    "mongoose": "^8.17.1",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@nx/eslint": "21.3.11",
    "@nx/eslint-plugin": "21.3.11",
    "@nx/js": "21.3.11",
    "@nx/nest": "21.3.11",
    "@nx/node": "21.3.11",
    "@nx/web": "21.3.11",
    "@nx/webpack": "21.3.11",
    "@nx/workspace": "21.3.11",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/express": "^4.17.23",
    "@types/multer": "^2.0.0",
    "@types/node": "~18.16.9",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "21.3.11",
    "prettier": "^2.6.2",
    "prisma": "^6.14.0",
    "tslib": "^2.3.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0",
    "webpack-cli": "^5.1.4"
  },
  "workspaces": [
    "apps/*",
    "libs/shared/types",
    "libs/shared/helpers",
    "libs/shared/core",
    "libs/shared/config/user/config-user",
    "libs/shared/post/models",
    "libs/shared/config/file-storage/file-storage-config",
    "libs/shared/config/notification/notification-config",
    "libs/post/*",
    "libs/share/post/*",
    "libs/shared/post/*",
    "libs/shared/config/*",
    "libs/shared/config/post/*"
  ]
}
